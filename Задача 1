#include <iostream>
#include <limits.h>
using namespace std;
 
void OutputArray(int **a, int n)
{
	for (int i=0; i<n; i++)
	{
		for (int j=0; j<n; j++)
			cout << a[i][j] << " ";
		cout << endl;
	}
}
 
int ** InitArray(int n)
{
	int **a = new int*[n];
	for (int i=0; i<n; i++)
	{
		a[i] = new int[n];
		for (int j=0; j<n; j++)
			a[i][j] = 0;
	}
	return a;
}
 
int main() {
	int n;
	cin >> n;
	int a[n][2];
	;
}
