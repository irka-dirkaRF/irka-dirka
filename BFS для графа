#include <iostream>
#include <queue>
using namespace std;
 
void BFS(int **a, int n)
{
	queue<int> *q = new queue<int>();
	bool *used = new bool[n];
	for (int i=0; i<n; i++)
		used[i] = false;
	q -> push(0);
	while (q -> size() != 0)
	{
		int index = q -> front();
		q -> pop();
		for (int i=0; i<n; i++)
			if (a[index][i] != 0 && !used[i])
				q -> push(i);
		used[index] = true;
	}
}
 
int main() {
	int n;
	cin >> n;
	int **a = new int*[n];
	for (int i=0; i<n; i++)
		a[i] = new int[n];
	for (int i=0; i<n; i++)
		for (int j=0; j<n; j++)
			cin >> a[i][j];
	BFS(a, n);
	return 0;
}
